{
    "compilerOptions": {
      "target": "es6",
      "module": "es6",
      "outDir": "dist/",
      "sourceMap": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "skipLibCheck": true
    },
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "typings/*",
      "node_modules"
    ]
  }